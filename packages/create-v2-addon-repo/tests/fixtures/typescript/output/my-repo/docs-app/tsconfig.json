{
  "extends": "@shared-configs/typescript/ember",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "docs-app/tests/*": ["tests/*"],
      "docs-app/*": ["app/*"],
      "*": ["types/*"]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "@glint/ember-tsc/types",
      "vite/client"
    ]
  },
  "include": ["app/**/*", "tests/**/*", "types/**/*"]
}
